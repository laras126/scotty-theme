{% set img = TimberImage(id) %}

<img
  data-sizes="auto"
  src="data:image/gif;base64,R0lGODlhAQABAAAAACH5BAEKAAEALAAAAAABAAEAAAICTAEAOw=="
  data-src="{{img.src|resize(600)}}"
  data-srcset="
      {{img.src|resize(400)}} 300w,
      {{img.src|resize(600)}} 600w,
      {{img.src|resize(900)}} 900w"
  class="lazyload blur-up {{class}}" />
